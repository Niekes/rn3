{
  "name": "rn3",
  "version": "0.0.5",
  "description": "Data Visualization Toolkit",
  "private": false,
  "scripts": {
    "test": "c8 --reporter=lcov tape ./tests/setup.mjs ./tests/**/*-test.mjs | tap-spec",
    "coverage": "c8 tape ./tests/setup.mjs ./tests/**/*-test.mjs | tap-spec && c8 report",
    "build": "rm -rf src/rn3.*.js && rm -rf src/rn3.*.css && webpack --mode=production",
    "reset": "rm package-lock.json || true && rm -rf node_modules/ || true && npm install && npm dedupe && npm test",
    "prepack": "npm run build",
    "start": "webpack --mode=development"
  },
  "homepage": "https://github.com/niekes/rn3",
  "repository": {
    "type": "git",
    "url": "https://github.com/niekes/rn3"
  },
  "bugs": {
    "url": "https://github.com/niekes/rn3/issues"
  },
  "keywords": [
    "visualization",
    "3d",
    "svg",
    "canvas"
  ],
  "type": "module",
  "author": "Stefan Nieke",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-env": "^3.3.0",
    "c8": "^7.6.0",
    "canvas": "^2.7.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "sass-recursive-map-merge": "^1.0.1",
    "style-loader": "^2.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.2",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "yargs": "^12.0.5"
  },
  "files": [
    "index.scss",
    "index.js",
    "styles/*",
    "src/*"
  ],
  "dependencies": {
    "d3": "^7.2.1"
  }
}
